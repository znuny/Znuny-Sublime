${1:my @Attachments = \$ParserObject->GetAttachments();
for my \$Attachment (@Attachments) \{
    print \$Attachment->\{Filename\};
    print \$Attachment->\{Charset\};
    print \$Attachment->\{MimeType\};
    print \$Attachment->\{ContentType\};
    print \$Attachment->\{Content\};
}
